{
  "name": "faker-br",
  "description": "Brazilian Faker Data Generator - Gerador de dados para o Brasil Fork do faker para pt_BR com extens√£o para CNPJ, CPF, RG, etc.",
  "version": "0.1.2",
  "contributors": [
    "Tamnil Saito Junior <tamnil@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/tamnil/Faker-br.git"
  },
  "scripts": {
    "lint": "node_modules/.bin/jshint ./lib --config ./.jshintrc",
    "test": "node_modules/.bin/mocha test/*.*.js",
    "build": "cd build && ../node_modules/.bin/gulp && cd ../",
    "doc": "jsdoc -c conf.json -t ./node_modules/ink-docstrap/template -R README.md lib",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "all": false,
    "include": [
      "lib/**/*.js",
      "vendor/*.js"
    ],
    "exclude": [
      "coverage",
      "locales",
      "modules",
      "white",
      "docs",
      "docker",
      "public",
      "reports",
      "tests",
      "node_modules"
    ],
    "reporter": [
      "html",
      "lcov",
      "clover"
    ],
    "report-dir": "./reports/coverage"
  },
  "devDependencies": {
    "browserify": "5.11.1",
    "coveralls": "^3.0.2",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-jsdoc3": "^0.2.1",
    "gulp-mustache": "0.4.0",
    "gulp-rename": "1.4.0",
    "gulp-uglify": "1.0.1",
    "ink-docstrap": "1.3.2",
    "jsdoc": "^3.4.0",
    "jshint": "2.9.7",
    "lodash": "^4.6.1",
    "mocha": "^5.2.0",
    "node-minify": "*",
    "optimist": "0.6.1",
    "sinon": "^7.2.2",
    "through2": "2.0.0",
    "vinyl-transform": "1.0.0"
  },
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "nyc": "^13.1.0"
  }
}
